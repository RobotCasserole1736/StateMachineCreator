{
  "name": "cass-sm-editor",
  "description": "Casserole State Machine Editor",
  "version": "0.0.1",
  "publisher": "FRC1736",
  "engines": {
    "vscode": "^1.23.0"
  },
  "activationEvents": [
    "onCommand:SMEditor.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "SMEditor.start",
        "title": "Start new state machine coding session",
        "category": "SM Editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint . --ext .ts,.tsx",
    "watch": "tsc -w -p ./"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "eslint": "^7.1.0",
    "typescript": "^3.9.4",
    "@types/vscode": "^1.38.0"
  },
  "author": "",
  "license": "ISC"
}
